                    </group>
                </group> 
                <notebook>
                    <page string="Purchase Order Lines">
                        <field name="purchase_order_line_ids">
                            <tree editable="bottom">
                                <field name="ItemId" />
                                <field name="serialNo"/>
                                <field name="supplier"/>
                                <field name="code"/>
                                <field name="description"/>
                                <field name="longDescription"/>
                                <field name="uomCode"/>
                                <field name="quantity"/>
                                <field name="rate"/>
                                <field name="amount"/>
                                <field name="subProject"/>
                                <field name="budgetHead"/>
                            </tree>
                        </field>
                    </page>
                    <page string="Product Lines">
                        <field name="vj_po_product_detail_line_ids">
                            <tree editable="bottom">
                                <field name="poid" invisible="1" />
                                <field name="ledger_description"/>
                                <field name="description"/>
                                <field name="uom_code"/>
                                <field name="total_quantity"/>
                                <field name="pending_quantity"/>
                                <field name="submitted_quantity"/>
                            </tree>
                        </field>
                    </page>
                </notebook>                   
            </sheet>
        </form>
    </field>
</record>


