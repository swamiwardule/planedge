    <record id="sbu_tree_id" model="ir.ui.view">
        <field name="name">sbu.tree</field>
        <field name="model">sub.business.unit</field>
        <field name="priority">4</field>
        <field name="arch" type="xml">
            <tree string="Sub Business Unit">
                <field name="description"/>
                <field name="type"/>
                <field name="parentId"/>
                <field name="segmentId"/>
                <field name="subDescription"/>
                <field name="subId"/>
                <field name="buId"/>
                <field name="vjd_bu_hie_id"/>
                <field name="vjd_pro_hie_id"/>
                <field name="project_state"/>
                <field name="tower_state"/>
            </tree>
        </field>
    </record>

        <record model="ir.ui.view" id="sbu_form_id">
            <field name="name">sbu.form</field>
            <field name="model">sub.business.unit</field>
            <field name="arch" type="xml">
                <form>
                  <sheet>
                    <group>
                        <group>
                         <field name="description"/>
                            <field name="type"/>
                            <field name="parentId"/>
                            <field name="segmentId"/>
                            <field name="subDescription"/>
                        </group>
                        <group>
                    
                            <field name="subId"/>
                            <field name="buId"/>
                            <field name="vjd_bu_hie_id"/>
                            <field name="vjd_pro_hie_id"/>
                            <field name="tower_type" readonly="0"/>
                        </group>
                           <group>
                            <field name="project_state"/>
                            <field name="tower_state"/>
                            <field name="ff_state" attrs="{'invisible': [('tower_type', '=', 'development')]}"/>
                        </group>
                            <group>
                                <field name="project_info_id"/>
                                <field name="tower_id"/>
                            </group>
                        <group>
                            <button name="create_project_towers" string="Create Project/Tower" type="object"  class="btn-primary"/>
                            <button name="create_flats_floors" string="Create Flat/Floor" type="object" class="btn btn-primary me-1" attrs="{'invisible': [('tower_type', '=', 'development')]}"/>
                        </group>
                    </group>
                </sheet>
                  <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
                </form>
            </field>
        </record>

