  <record model="ir.ui.view" id="vjpo_form_id">
    <field name="name">vj.purchase.order.form</field>
    <field name="model">vj.purchase.order</field>
    <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <group col="2">
                        <field name="pid"/>
                        <field name="buId"/>
                        <field name="businessUnitDescription"/>
                        <field name="documentNo"/>
                        <field name="documentDate"/>
                        <field name="ledgerId"/>
                        <field name="ledgerDescription"/>
