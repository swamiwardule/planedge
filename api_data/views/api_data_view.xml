<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="api_log_tree_id" model="ir.ui.view">
        <field name="name">api.log.tree</field>
        <field name="model">api.log</field>
        <field name="priority">4</field>
        <field name="arch" type="xml">
            <tree string="Logs">
                <field name="name"/>
                <field name="api_name"/>
                <field name="create_datetime"/>
                <field name="state"/>
                <field name="error"/>
                <field name="message"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="api_log_form_id">
        <field name="name">api.log.form</field>
        <field name="model">api.log</field>
        <field name="arch" type="xml">
            <form>
            <sheet>
                <group>
                    <group col="2">
                       <field name="name" readonly="1"/>
                        <field name="api_name" readonly="1"/>
                        <field name="create_datetime" readonly="1"/>
                        <field name="state" readonly="1"/>
                        <field name="error" readonly="1"/>
                        <field name="message" readonly="1"/>
                    </group>     
                </group>                    
            </sheet>
            </form>
        </field>
    </record>

        <record id="action_api_log" model="ir.actions.act_window">
        <field name="name">APi Logs</field>
        <field name="res_model">api.log</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="api_log_tree_id"/>
        </record>

    <record id="bu_tree_id" model="ir.ui.view">
        <field name="name">bu.tree</field>
